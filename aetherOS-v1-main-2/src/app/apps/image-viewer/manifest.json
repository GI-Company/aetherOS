
{
  "id": "system.image.viewer",
  "name": "Image Viewer",
  "version": "1.0.0",
  "developer": "AetherOS",
  "description": "A simple viewer for image files.",
  "entry": "index.tsx",
  "permissions": {
    "filesystem_read": true
  },
  "sandbox": {
    "profile": "ui"
  },
  "signing": {
    "publicKey": "",
    "fingerprint": ""
  },
  "ui_hints": {
    "resizable": true,
    "theme": "glassmorphism",
    "defaultSize": { "width": 800, "height": 600 },
    "hideFromDock": true
  }
}
