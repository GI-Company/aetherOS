
{
  "id": "system.code.editor",
  "name": "Code Editor",
  "version": "1.0.0",
  "developer": "AetherOS",
  "description": "An AI-powered code editor with cloud file system integration.",
  "entry": "index.tsx",
  "permissions": {
    "filesystem_read": true,
    "filesystem_write": true,
    "ai_access": true
  },
  "sandbox": {
    "profile": "ui"
  },
  "signing": {
    "publicKey": "",
    "fingerprint": ""
  },
  "ui_hints": {
    "resizable": true,
    "theme": "glassmorphism",
    "defaultSize": { "width": 800, "height": 600 }
  }
}
