
{
  "id": "system.vm.terminal",
  "name": "Virtual Machine",
  "version": "1.0.0",
  "developer": "AetherOS",
  "description": "A natural language shell for interacting with the AI agent and running WASM binaries.",
  "entry": "index.tsx",
  "permissions": {
    "ai_access": true,
    "filesystem_read": true,
    "filesystem_write": true,
    "vm_run": true
  },
  "sandbox": {
    "profile": "ui"
  },
  "signing": {
    "publicKey": "",
    "fingerprint": ""
  },
  "ui_hints": {
    "resizable": true,
    "theme": "glassmorphism",
    "defaultSize": { "width": 640, "height": 480 }
  }
}
