
{
  "id": "system.file.explorer",
  "name": "File Explorer",
  "version": "1.0.0",
  "developer": "AetherOS",
  "description": "Browse and manage your cloud file system.",
  "entry": "index.tsx",
  "permissions": {
    "filesystem_read": true,
    "filesystem_write": true,
    "ai_access": true
  },
  "sandbox": {
    "profile": "ui"
  },
  "signing": {
    "publicKey": "",
    "fingerprint": ""
  },
  "ui_hints": {
    "resizable": true,
    "theme": "glassmorphism",
    "defaultSize": { "width": 700, "height": 500 }
  }
}
